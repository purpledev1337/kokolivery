<div v-if="catAntipasto.length" class="category_box">
        <h2>Antipasti:</h2>
        <div class="dish_box" v-for="dish in catAntipasto" :key="dish.id + dish.name">
          <div class="dish_card" v-if="dish.is_visible || dish.delete">
            <img
              src="data:image/jpeg;base64,"
              alt=""
            />
            <!-- <img :src="require(`../public/storage/${dish.image_path}`)" alt=""> -->
            <h2>{{ dish.id }}</h2>
            <h2>{{ dish.name }}</h2>
            <h3>Prezzo: {{ dish.price }}</h3>
            <h3>{{ dish.description }}</h3>
            <span>({{ dish.category }})</span>
            <br>

            <span class="btn btn-primary" 
                  v-if="cart.some((item) => item.id === dish.id)" 
                  @click="addToCart(dish)"> + 
            </span>

            <!-- <span v-if="cart.some((item) => item.id === dish.id)" >
              {{ quantitySingle(dish.id) }}
            </span> -->

            <span class="btn btn-primary" 
                  v-else
                  @click="addToCart(dish)"> Aggiungi al carrello 
            </span>

            <span
              class="btn btn-danger"
              @click="removeFromCart(dish)"
              v-if="cart.some((item) => item.id === dish.id)"
            >
              -
            </span>
          </div>
        </div>
      </div>
